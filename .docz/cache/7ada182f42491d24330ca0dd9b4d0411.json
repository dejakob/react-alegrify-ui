K,CAACI,QALpB;AAMI,IAAA,OAAO,EAAEJ,KAAK,CAACQ,OANnB;AAOI,IAAA,QAAQ,EAAER,KAAK,CAACS,QAPpB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IAHJ,EAYI;AACI,IAAA,OAAO,EAAET,KAAK,CAACM,EADnB;AAEI,IAAA,SAAS,EAAC,0BAFd;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAII;AACI,IAAA,SAAS,EAAC,0BADd;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAGKN,KAAK,CAACU,QAHX,CAJJ,CAZJ,CADJ;AAyBH;;AAEDX,QAAQ,CAACY,SAAT,GAAqB;AAEjB;;;AAGAL,EAAAA,EAAE,EAAET,SAAS,CAACe,MAAV,CAAiBC,UALJ;;AAOjB;;;AAGAN,EAAAA,IAAI,EAAEV,SAAS,CAACe,MAAV,CAAiBC,UAVN;;AAYjB;;;AAGAH,EAAAA,QAAQ,EAAEb,SAAS,CAACiB,SAAV,CAAoB,CAC1BjB,SAAS,CAACkB,OAAV,CAAkBlB,SAAS,CAACmB,IAA5B,CAD0B,EAE1BnB,SAAS,CAACmB,IAFgB,CAApB,EAGPH,UAlBc;;AAoBjB;;;AAGAT,EAAAA,QAAQ,EAAEP,SAAS,CAACoB,IAvBH;;AAyBjB;;;AAGAT,EAAAA,OAAO,EAAEX,SAAS,CAACoB,IA5BF;;AA8BjB;;;AAGAd,EAAAA,SAAS,EAAEN,SAAS,CAACe;AAjCJ,CAArB;AAmCAb,QAAQ,CAACmB,YAAT,GAAwB;AACpBZ,EAAAA,EAAE,EAAE,UADgB;AAEpBC,EAAAA,IAAI,EAAE,UAFc;AAGpBG,EAAAA,QAAQ,EAAE,uBAHU;AAIpBN,EAAAA,QAAQ,EAAE,KAJU;AAKpBI,EAAAA,OAAO,EAAE,KALW;AAMpBL,EAAAA,SAAS,EAAE;AANS,CAAxB;;eASeL,oBAAoB,CAACC,QAAD,C;;AAAnC;;;;;;;;;;0BAhFSA,Q","sourcesContent":["import React from 'react';\nimport PropTypes from 'prop-types';\nimport { attachUniversalProps } from './helpers/universal-props';\n\n/**\n * <Checkbox />\n * \n * ```jsx\n * <Checkbox\n *  id=\"my_checkbox\"\n *  name=\"my_checkbox\"\n *  checked={false}\n * >\n *   I agree to GDPR, cookie banners, my local law and the fact that bears have feelings\n * </Checkbox>\n * ```\n */\nfunction Checkbox(props) {\n    const classNames = ['alegrify-checkbox'];\n\n    classNames.push(props.className);\n\n    if (props.disabled) {\n        classNames.push('alegrify-checkbox--disabled');\n    }\n\n    return (\n        <div\n            className={classNames.join(' ')}\n        >\n            <input\n                type=\"checkbox\"\n                id={props.id}\n                name={props.name}\n                className=\"alegrify-checkbox__trigger\"\n                disabled={props.disabled}\n                checked={props.checked}\n                onChange={props.onChange}\n            />\n            <label\n                htmlFor={props.id}\n                className=\"alegrify-checkbox__check\"\n            >\n                <span \n                    className=\"alegrify-checkbox__label\"\n                >\n                    {props.children}\n                </span>\n            </label>\n        </div>\n    );\n}\n\nCheckbox.propTypes = {\n\n    /**\n     * input id\n     */\n    id: PropTypes.string.isRequired,\n\n    /**\n     * input name\n     */\n    name: PropTypes.string.isRequired,\n\n    /**\n     * Label content checkbox\n     */\n    children: PropTypes.oneOfType([\n        PropTypes.arrayOf(PropTypes.node),\n        PropTypes.node\n    ]).isRequired,\n\n    /**\n     * Is checkbox disabled?\n     */\n    disabled: PropTypes.bool,\n\n    /**\n     * Is checkbox checked?\n     */\n    checked: PropTypes.bool,\n\n    /**\n     * Additional classnames\n     */\n    className: PropTypes.string\n};\nCheckbox.propExamples = {\n    id: 'checkbox',\n    name: 'checkbox',\n    children: 'Look at this checkbox',\n    disabled: false,\n    checked: false,\n    className: ''\n};\n\nexport default attachUniversalProps(Checkbox);\n"]}]}